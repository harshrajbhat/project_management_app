<div id="<%= dom_id team %>">
  <p>
    <strong>Name:</strong>
    <%= team.name %>
  </p>

  <p>
    <strong>Project:</strong>
    <%= team.project_id %>
  </p>

  <h3>Team Members</h3>
  <ul>
    <% team.users.each do |user| %>
      <li><%= user.name %> (<%= user.email %>)</li>
    <% end %>
  </ul>

</div>

